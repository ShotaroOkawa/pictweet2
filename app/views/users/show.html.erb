<div class="contents row">
  <p><%= @nickname %>さんの投稿一覧</p>
  <% @tweets.each do |tweet| %>
    <div class="content_post" style="background-image: url(<%= tweet.image %>);">
      <p><%= tweet.text %></p>
      <a href="/users/<%= tweet.user_id %>">
        <span class="name"><%= tweet.user.nickname %></span>
      </a>
    </div>
  <% end %>
</div>